    <d:mapping name="f1511" parameters="value">
      <d:step value="true" when="value &lt;= 41.500" />
      <d:step value="false" when="value &lt;= 42.500" />
      <d:step value="true" when="value &lt;= 45.500" />
      <d:step value="false" when="value &lt;= 46.500" />
      <d:otherwise value="true" />
    </d:mapping>
    <d:mapping name="f1512" parameters="value">
