      <d:step value="[[colors[42], colors[19]], ['none', 'none']]" when="value &lt;= 42.500" />
      <d:step value="['rgb(255, 255, 255)', 'none']" when="value &lt;= 45.500" />
      <d:step value="[[colors[46], colors[19]], ['none', 'none']]" when="value &lt;= 46.500" />
      <d:otherwise value="['rgb(255, 255, 255)', 'none']" />
    </d:mapping>
    <d:mapping name="f1513" parameters="value">
