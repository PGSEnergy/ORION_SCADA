<?xml version="1.0" encoding="utf-8"?>
<d:document nature="http://www.w3.org/2000/svg" xmlns:n="http://www.webui-ap.com/schemata/naming/1.0" xmlns:s="http://www.w3.org/2000/svg" xmlns:y="http://www.webui-ap.com/schemata/styling/1.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:i18n="http://www.webui-ap.com/schemata/i18n/1.0" i18n:translate="false" xmlns:d="http://www.webui-ap.com/schemata/view/1.0">
  <d:head>
    <d:use name="power-process-symbols" />
    <d:variable type="external" name="auth-group" default="''" />
    <d:variable type="external" name="auth-level" default="''" />
    <d:variable type="external" name="subdrawing_color" default="'rgb(255, 255, 255)'" />
    <d:variable type="external" name="subdrawing_dynamics" default="1" />
    <d:variable type="external" name="control_action_enabled" default="1" />
    <d:meta name="SAVersionInfo" content="1" />
    <d:meta name="dv-custom-attribute|SAObjectType" content="Alarm Indicator" />
    <d:meta name="dv-custom-attribute|SARepresentation" content="Led" />
    <d:meta name="dv-custom-attribute|SANoRotate" content="" />
    <d:meta name="dv-custom-attribute|SAVersionInfo" content="1" />
    <d:meta name="dv-custom-attribute|SAAuthLevel" content="1" />
    <d:meta name="dv-custom-attribute|SAOpenVSCILOnClickVso" content="ooooo@1jjjj@2&quot;xxx&quot;" />
    <d:meta name="dv-custom-attribute|SAAddCustomAttribute" content="xxx" />
    <d:role name="symbol" />
    <d:variable type="external" name="state" default="0" />
    <d:variable type="external" name="status" default="0" />
    <d:variable type="external" name="led-color" default="0" />
    <d:variable type="external" name="text" default="''" />
    <d:variable type="external" name="ss" default="0" />
    <d:mapping name="f0" parameters="value">
      <d:step value="colors[31]" when="value &lt;= 0.500" />
      <d:step value="'rgb(0, 0, 255)'" when="value &lt;= 1.500" />
      <d:otherwise value="colors[31]" />
    </d:mapping>
    <d:mapping name="f1" parameters="value">
      <d:step value="true" when="value &lt;= 0.500" />
      <d:step value="false" when="value &lt;= 1.500" />
      <d:otherwise value="true" />
    </d:mapping>
    <d:mapping name="f2" parameters="value">
      <d:step value="colors[31]" when="value &lt;= 34.500" />
      <d:step value="'rgb(255, 0, 255)'" when="value &lt;= 35.500" />
      <d:step value="colors[31]" when="value &lt;= 37.500" />
      <d:step value="'rgb(255, 0, 255)'" when="value &lt;= 39.500" />
      <d:step value="colors[31]" when="value &lt;= 49.500" />
      <d:step value="'rgb(255, 0, 255)'" when="value &lt;= 50.499" />
      <d:otherwise value="colors[31]" />
    </d:mapping>
    <d:mapping name="f3" parameters="value">
      <d:step value="true" when="value &lt;= 34.500" />
      <d:step value="false" when="value &lt;= 35.500" />
      <d:step value="true" when="value &lt;= 37.500" />
      <d:step value="false" when="value &lt;= 39.500" />
      <d:step value="true" when="value &lt;= 49.500" />
      <d:step value="false" when="value &lt;= 50.499" />
      <d:otherwise value="true" />
    </d:mapping>
    <d:mapping name="f4" parameters="value">
      <d:step value="true" when="value &lt;= 23.500" />
      <d:step value="false" when="value &lt;= 25.500" />
      <d:step value="true" when="value &lt;= 26.500" />
      <d:step value="false" when="value &lt;= 28.500" />
      <d:step value="true" when="value &lt;= 45.499" />
      <d:step value="false" when="value &lt;= 46.499" />
      <d:step value="true" when="value &lt;= 47.499" />
      <d:step value="false" when="value &lt;= 48.499" />
      <d:otherwise value="true" />
    </d:mapping>
    <d:mapping name="f5" parameters="value">
      <d:step value="[[colors[31], 'rgb(255, 0, 0)'], ['none', 'none']]" when="value &lt;= 1.500" />
      <d:step value="[[colors[31], 'rgb(255, 224, 0)'], ['none', 'none']]" when="value &lt;= 2.500" />
      <d:step value="[[colors[31], colors[72]], ['none', 'none']]" when="value &lt;= 3.500" />
      <d:step value="[[colors[31], colors[104]], ['none', 'none']]" when="value &lt;= 4.500" />
      <d:otherwise value="[[colors[31], 'rgb(255, 255, 255)'], ['none', 'none']]" />
    </d:mapping>
    <d:mapping name="f6" parameters="value">
      <d:step value="'rgb(255, 0, 0)'" when="value &lt;= 1.500" />
      <d:step value="'rgb(255, 224, 0)'" when="value &lt;= 2.500" />
      <d:step value="colors[72]" when="value &lt;= 3.500" />
      <d:step value="colors[104]" when="value &lt;= 4.500" />
      <d:otherwise value="'rgb(255, 255, 255)'" />
    </d:mapping>
    <d:mapping name="f7" parameters="value">
      <d:step value="true" when="value &lt;= 0.500" />
      <d:step value="false" when="value &lt;= 1.500" />
      <d:otherwise value="true" />
    </d:mapping>
    <d:mapping name="f8" parameters="value">
      <d:step value="'rgb(0, 0, 0)'" when="value &lt;= 0.500" />
      <d:step value="'rgb(0, 0, 255)'" when="value &lt;= 1.500" />
      <d:otherwise value="'rgb(0, 0, 0)'" />
    </d:mapping>
    <d:mapping name="f9" parameters="value">
      <d:step value="1" when="value &lt;= 0.500" />
      <d:step value="3" when="value &lt;= 1.500" />
      <d:otherwise value="1" />
    </d:mapping>
  </d:head>
  <d:body x1="327.680" y1="327.680" x2="-327.680" y2="-327.680">
    <s:rect width="10" height="10" x="-5" y="-5" s:pointer-events="visiblePainted" y:fill="{colors[31]}" y:fill-rule="evenodd" y:stroke="{colors[17]}" y:vector-effect="non-scaling-stroke" y:stroke-width="1" d:object-name="Back_Box" />
    <s:rect width="10" height="10" x="-5" y="-5" s:pointer-events="visiblePainted" d:transform-anchor="center-center" d:hidden="f1(`ss`) and `subdrawing_dynamics` = 1" y:fill="{if(`subdrawing_dynamics`, f0(`ss`), subdrawing_color)}" y:fill-rule="evenodd" y:stroke="none" y:vector-effect="non-scaling-stroke" y:stroke-width="1" d:object-name="Box_blue" />
    <s:rect width="10" height="10" x="-5" y="-5" s:pointer-events="visiblePainted" d:transform-anchor="center-center" d:hidden="f3(`status`) and `subdrawing_dynamics` = 1" y:fill="{if(`subdrawing_dynamics`, f2(`status`), subdrawing_color)}" y:fill-rule="evenodd" y:stroke="none" y:vector-effect="non-scaling-stroke" y:stroke-width="1" d:object-name="Box_pink" />
    <s:polyline points="-5,5 -5,-5 5,-5 " y:fill="none" y:fill-rule="evenodd" y:stroke="{colors[16]}" y:vector-effect="non-scaling-stroke" y:stroke-width="2" d:object-name="Frame Top Left" />
    <s:polyline points="-5,5 5,5 5,-5 " y:fill="none" y:fill-rule="evenodd" y:stroke="{colors[17]}" y:vector-effect="non-scaling-stroke" y:stroke-width="3" d:object-name="Frame Bottom Right" />
    <s:rect width="10" height="10" x="-5" y="-5" s:pointer-events="visiblePainted" d:hidden="f4(`status`) and `subdrawing_dynamics` = 1" d:transform-anchor="center-center" y:fill="{colors[31]}" y:fill-rule="evenodd" y:stroke="none" y:vector-effect="non-scaling-stroke" y:stroke-width="1" d:object-name="Box_alarm">
      <d:animate attributes="fill, stroke" values="if(`subdrawing_dynamics`, f5(`led-color`), colors[31])" />
    </s:rect>
    <s:text font-family="Times New Roman" font-size="14.767" fill="{colors[30]}" x="6.440" y="0" text-anchor="start" dominant-baseline="central" dx="1.477px" d:textScaleAdjustment="true" d:transform-anchor="center-center" y:fill="{colors[30]}" y:fill-rule="evenodd">{if(`subdrawing_dynamics`, format('%s', `text`), 'XXXXXXXXXXXXXXXXXXXX')}</s:text>
    <s:circle cx="0" cy="0" r="4.300" y:fill="{colors[31]}" y:fill-rule="evenodd" y:stroke="{colors[17]}" y:vector-effect="non-scaling-stroke" y:stroke-width="1" d:object-name="LED_main" />
    <s:circle cx="0" cy="0" r="4.300" d:transform-anchor="center-center" d:hidden="f7(`state`) and `subdrawing_dynamics` = 1" y:fill="{if(`subdrawing_dynamics`, f6(`led-color`), subdrawing_color)}" y:fill-rule="evenodd" y:stroke="none" y:vector-effect="non-scaling-stroke" y:stroke-width="1" d:object-name="LED_main" />
    <s:circle cx="0" cy="0" r="4.300" d:transform-anchor="center-center" y:stroke-width="{f9(`ss`)}" y:fill="none" y:fill-rule="evenodd" y:stroke="{if(`subdrawing_dynamics`, f8(`ss`), 'rgb(0, 0, 0)')}" y:vector-effect="non-scaling-stroke" d:object-name="Led_sec" />
  </d:body>
</d:document>