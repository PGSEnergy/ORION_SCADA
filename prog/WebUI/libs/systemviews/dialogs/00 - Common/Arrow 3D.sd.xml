<?xml version="1.0" encoding="utf-8"?>
<d:document nature="http://www.w3.org/2000/svg" xmlns:n="http://www.webui-ap.com/schemata/naming/1.0" xmlns:s="http://www.w3.org/2000/svg" xmlns:y="http://www.webui-ap.com/schemata/styling/1.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:i18n="http://www.webui-ap.com/schemata/i18n/1.0" i18n:translate="false" xmlns:d="http://www.webui-ap.com/schemata/view/1.0">
  <d:head>
    <d:use name="power-process-symbols" />
    <d:variable type="external" name="auth-group" default="''" />
    <d:variable type="external" name="auth-level" default="''" />
    <d:variable type="external" name="subdrawing_color" default="'rgb(255, 255, 255)'" />
    <d:variable type="external" name="subdrawing_dynamics" default="1" />
    <d:variable type="external" name="control_action_enabled" default="1" />
    <d:meta name="dv-custom-attribute|ABB SA Symbols" content="v1.0" />
    <d:meta name="dv-custom-attribute|SAObjectType" content="Static Arrow 3D" />
    <d:meta name="dv-custom-attribute|SASuperType" content="Static" />
    <d:meta name="dv-custom-attribute|SATopology" content="" />
    <d:role name="symbol" />
    <d:variable type="external" name="horizontal" default="0" />
    <d:mapping name="f0" parameters="value">
      <d:step value="'url(#i2)'" when="value &lt;= 0.500" />
      <d:otherwise value="'url(#i3)'" />
    </d:mapping>
  </d:head>
  <d:body x1="327.680" y1="327.680" x2="-327.680" y2="-327.680">
    <s:rect width="0" height="0" x="0" y="9" s:pointer-events="visiblePainted" y:fill="none" y:fill-rule="evenodd" y:stroke="{subdrawing_color}" y:vector-effect="non-scaling-stroke" y:stroke-width="1" s:visibility="hidden" d:object-name="Hotspot1" />
    <s:linearGradient x1="0%" y1="0%" x2="100%" y2="0%" id="i2" d:adjustToScreenCoordSystem="true">
      <s:stop stop-color="{colors[16]}" offset="0" />
      <s:stop stop-color="{subdrawing_color}" offset="1" />
    </s:linearGradient>
    <s:linearGradient x1="0%" y1="0%" x2="0%" y2="100%" id="i3" d:adjustToScreenCoordSystem="true">
      <s:stop stop-color="{colors[16]}" offset="0" />
      <s:stop stop-color="{subdrawing_color}" offset="1" />
    </s:linearGradient>
    <s:linearGradient x1="0%" y1="0%" x2="100%" y2="0%" id="i4" d:adjustToScreenCoordSystem="true">
      <s:stop stop-color="{colors[16]}" offset="0" />
      <s:stop stop-color="{subdrawing_color}" offset="1" />
    </s:linearGradient>
    <s:polygon points="10,10 0,0 -10,10 10,10 " y:fill="{if(`subdrawing_dynamics`, f0(`horizontal`), subdrawing_color)}" y:fill-rule="evenodd" d:transform-anchor="center-center" y:stroke="none" y:vector-effect="non-scaling-stroke" y:stroke-width="1" />
  </d:body>
</d:document>