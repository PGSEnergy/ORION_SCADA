<?xml version="1.0" encoding="UTF-8"?>
<LOG_CFG>
   <LogControl>
       <LogCommon>
           <LogElapsedTime>Off</LogElapsedTime>
       </LogCommon>
       <LogFileAttributes>
           <LogFileEnable>On</LogFileEnable>
           <LogFileName>\sc\prog\iccp\IccpTrace.log</LogFileName>
           <LogFileSize>1000000</LogFileSize>
           <DestroyOldFile>On</DestroyOldFile>
           <HardFlush>Off</HardFlush>
       </LogFileAttributes>
       <LogIpcAttributes>
           <LogIpcAppId>SiteB Primary</LogIpcAppId>
           <LogIpcMaxQueCount>100</LogIpcMaxQueCount>
           <LogIpcListenEnable>Off</LogIpcListenEnable>
           <LogIpcListenPort>21223</LogIpcListenPort>
           <LogIpcNumListenPorts>1</LogIpcNumListenPorts>
           <LogIpcMaxListenConn>1</LogIpcMaxListenConn>
           <LogIpcCallEnable>On</LogIpcCallEnable>
           <LogIpcCallingPort>21001</LogIpcCallingPort>
           <LogIpcCallingIp>127.0.0.1</LogIpcCallingIp>
           <LogIpcCallingBackoff>500</LogIpcCallingBackoff>
           <LogIpcSmartMode>On</LogIpcSmartMode>
           <LogIpcEditLogCfg>On</LogIpcEditLogCfg>
           <LogIpcSealMode>2</LogIpcSealMode>
           <LogIpcSealTimeWindow>10</LogIpcSealTimeWindow>
       </LogIpcAttributes>
   </LogControl>
   <LogMasks>
       <LOGCFG>
           <LOGCFG_NERR>On</LOGCFG_NERR>
           <LOGCFG_FLOW>Off</LOGCFG_FLOW>
           <!-- Log settings after parsing this file -->
           <LOGCFG_CFG>Off</LOGCFG_CFG>
       </LOGCFG>
       <SscLogMasks>
           <!-- SISCO Stack Configuration logging -->
           <SSC_LOG_NERR>On</SSC_LOG_NERR>
           <SSC_LOG_FLOW>Off</SSC_LOG_FLOW>
       </SscLogMasks>
       <UserLogMasks>
           <USER_LOG_CLIENT>Off</USER_LOG_CLIENT>
           <USER_LOG_SERVER>Off</USER_LOG_SERVER>
       </UserLogMasks>
       <MiLogMasks>
           <MI_LOG_NERR>On</MI_LOG_NERR>
           <MI_LOG_CFG>On</MI_LOG_CFG>
           <MI_LOG_ADDR>On</MI_LOG_ADDR>
           <MI_LOG_DISCOVERY>On</MI_LOG_DISCOVERY>
           <MI_LOG_FLOW>Off</MI_LOG_FLOW>
       </MiLogMasks>
       <MmsLogMasks>
           <MMS_LOG_NERR>On</MMS_LOG_NERR>
           <MMS_LOG_CLIENT>Off</MMS_LOG_CLIENT>
           <MMS_LOG_SERVER>Off</MMS_LOG_SERVER>
           <MMS_LOG_DEC>Off</MMS_LOG_DEC>
           <MMS_LOG_ENC>Off</MMS_LOG_ENC>
       </MmsLogMasks>
       <MvlLogMasks>
           <MVLLOG_NERR>On</MVLLOG_NERR>
           <MVLULOG_FLOW>Off</MVLULOG_FLOW>
           <MVLLOG_ACSE>Off</MVLLOG_ACSE>
           <MVLLOG_ACSEDATA>Off</MVLLOG_ACSEDATA>
       </MvlLogMasks>
       <SecurityLogMasks>
           <!--Off-->
           <SEC_LOG_NERR>On</SEC_LOG_NERR>
           <SEC_LOG_FLOW>Off</SEC_LOG_FLOW>
           <SEC_LOG_DATA>Off</SEC_LOG_DATA>
           <SEC_LOG_DEBUG>Off</SEC_LOG_DEBUG>
           <!--Off-->
           <SSLE_LOG_NERR>On</SSLE_LOG_NERR>
           <SSLE_LOG_FLOW>on</SSLE_LOG_FLOW>
           <SSLE_LOG_DATA>On</SSLE_LOG_DATA>
           <SSLE_LOG_DEBUG>On</SSLE_LOG_DEBUG>
           <SSLE_ASN1_LOG_NERR>On</SSLE_ASN1_LOG_NERR>
           <SSLE_ASN1_LOG_DEC>on</SSLE_ASN1_LOG_DEC>
           <SSLE_ASN1_LOG_ENC>on</SSLE_ASN1_LOG_ENC>
       </SecurityLogMasks>
       <RedundancyLogMasks>
           <RPL_LOG_NERR>Off</RPL_LOG_NERR>
           <RPL_LOG_FLOW>Off</RPL_LOG_FLOW>
           <IR_LOG_NERR>Off</IR_LOG_NERR>
           <IR_LOG_FLOW>Off</IR_LOG_FLOW>
           <IR_LOG_DATA>Off</IR_LOG_DATA>
       </RedundancyLogMasks>
       <AcseLogMasks>
           <ACSE_LOG_ENC>On</ACSE_LOG_ENC>
           <ACSE_LOG_DEC>On</ACSE_LOG_DEC>
           <COPP_LOG_DEC>Off</COPP_LOG_DEC>
           <COPP_LOG_DEC_HEX>Off</COPP_LOG_DEC_HEX>
           <COPP_LOG_ENC>Off</COPP_LOG_ENC>
           <COPP_LOG_ENC_HEX>Off</COPP_LOG_ENC_HEX>
           <COSP_LOG_DEC>Off</COSP_LOG_DEC>
           <COSP_LOG_DEC_HEX>Off</COSP_LOG_DEC_HEX>
           <COSP_LOG_ENC>Off</COSP_LOG_ENC>
           <COSP_LOG_ENC_HEX>Off</COSP_LOG_ENC_HEX>
       </AcseLogMasks>
       <TransportLogMasks>
           <TP_LOG_FLOWUP>Off</TP_LOG_FLOWUP>
           <TP_LOG_FLOWDOWN>Off</TP_LOG_FLOWDOWN>
       </TransportLogMasks>
       <SocketLogMasks>
           <SOCK_LOG_NERR>On</SOCK_LOG_NERR>
           <SOCK_LOG_FLOW>Off</SOCK_LOG_FLOW>
           <SOCK_LOG_TX>Off</SOCK_LOG_TX>
           <SOCK_LOG_RX>Off</SOCK_LOG_RX>
       </SocketLogMasks>
       <IpcLogMasks>
           <IPC_LOG_NERR>On</IPC_LOG_NERR>
           <IPC_LOG_FLOW>Off</IPC_LOG_FLOW>
           <IPC_LOG_DEBUG>Off</IPC_LOG_DEBUG>
           <IPC_LOG_TX>Off</IPC_LOG_TX>
           <IPC_LOG_RX>Off</IPC_LOG_RX>
       </IpcLogMasks>
       <SxLogMasks>
           <SX_LOG_NERR>On</SX_LOG_NERR>
           <SX_LOG_DEC>Off</SX_LOG_DEC>
       </SxLogMasks>
       <Asn1LogMasks>
           <ASN1_LOG_NERR>On</ASN1_LOG_NERR>
           <ASN1_LOG_DEC>Off</ASN1_LOG_DEC>
           <ASN1_LOG_ENC>Off</ASN1_LOG_ENC>
       </Asn1LogMasks>
       <MemLogMasks>
           <!-- Memory checking library module logging -->
           <MEM_LOG_CALLOC>Off</MEM_LOG_CALLOC>
           <MEM_LOG_MALLOC>Off</MEM_LOG_MALLOC>
           <MEM_LOG_REALLOC>Off</MEM_LOG_REALLOC>
           <MEM_LOG_FREE>Off</MEM_LOG_FREE>
       </MemLogMasks>
   </LogMasks>
   <MemAllocDbgCtrl>
       <!-- Memory debugging -->
       <MemFillEnable>Off</MemFillEnable>
       <MemHeapCheck>Off</MemHeapCheck>
       <MemCheckList>Off</MemCheckList>
       <MemFindNode>Off</MemFindNode>
       <MemNoReallocSmaller>Off</MemNoReallocSmaller>
   </MemAllocDbgCtrl>
</LOG_CFG>
